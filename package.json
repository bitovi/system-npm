{
  "name": "system-npm",
  "version": "0.5.9",
  "description": "npm plugin for StealJS and SystemJS",
  "main": "test/example-main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run test:browser && npm run test:live-reload",
    "test:browser": "grunt test",
    "live-reload-test": "cd test/live-reload && live-reload-test",
    "test:live-reload": "npm run live-reload-test & testee test/steal-live-reload.html --browsers firefox --reporter Spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stealjs/system-npm.git"
  },
  "author": "Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stealjs/system-npm/issues"
  },
  "homepage": "https://github.com/stealjs/system-npm",
  "devDependencies": {
    "bower": "^1.3.12",
    "can": "2.3.9",
    "grunt": "^0.4.5",
    "grunt-contrib-copy": "^0.8.0",
    "jquery": "2.1.3",
    "jquery-ui": "^1.10.5",
    "live-reload-testing": "^4.0.0",
    "lodash": "~2.4.1",
    "qunit": "~0.7.5",
    "steal": "^0.14.0",
    "steal-builtins": "^1.0.0",
    "steal-qunit": "stealjs/steal-qunit",
    "system-json": "0.0.2",
    "steal-es6-module-loader": "stealjs/es6-module-loader#v0.17.5",
    "steal-systemjs": "stealjs/systemjs#v0.17.4",
    "transpile": "stealjs/transpile#v0.9.4"
  },
  "system": {
    "npmAlgorithm": "flat",
    "npmIgnore": [
      "bower",
      "grunt",
      "testee",
      "systemjs",
      "steal",
      "qunit"
    ],
    "meta": {
      "./test/meta": {
        "format": "global",
        "exports": "foo"
      }
    }
  }
}
